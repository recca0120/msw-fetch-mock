{
  "name": "msw-fetch-mock",
  "version": "0.3.0",
  "description": "Undici-style fetch mock API built on MSW (Mock Service Worker)",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "msw",
    "fetch",
    "mock",
    "testing",
    "undici",
    "browser",
    "service-worker"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recca0120/msw-fetch-mock.git"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "source": "./src/node.ts",
      "types": {
        "import": "./dist/node.d.ts",
        "require": "./dist/node.d.cts"
      },
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./browser": {
      "source": "./src/browser.ts",
      "types": {
        "import": "./dist/browser.d.ts",
        "require": "./dist/browser.d.cts"
      },
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./legacy": {
      "source": "./src/legacy.ts",
      "types": {
        "import": "./dist/legacy.d.ts",
        "require": "./dist/legacy.d.cts"
      },
      "import": "./dist/legacy.js",
      "require": "./dist/legacy.cjs"
    }
  },
  "files": [
    "dist",
    "docs",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "prepare": "lefthook install",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "tsup"
  },
  "peerDependencies": {
    "msw": "^1.0.0 || ^2.12.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "lefthook": "^2.0.15",
    "msw": "^2.12.7",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.17"
  }
}
